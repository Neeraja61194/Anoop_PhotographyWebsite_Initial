extends ./layout.pug

block content
    .container
        p This is a form that you can use to upload a new image
        form(action="/photos" method="post" enctype="multipart/form-data")
            input(type="file" placeholder="Image" name="file")
            br
            select(name='category', id='' required="required")
                each val in folder_categoryList
                    option=val
            button Create